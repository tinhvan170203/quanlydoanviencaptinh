{
    "version": 2,
    "name": "phanmemquanlydoanvien",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/nodejs"
        }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}